<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="unahhennessy.com.suspend" >
    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="22" />
    <uses-permission android:name="android.permission.BLUETOOTH" />
    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN"/>
    <uses-permission android:name="android.permission.BLUETOOTH_PRIVILEGED"/>
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses.permission android:name="android.permission.CALL_PHONE"/>
    <uses.permission android:name="android.permission.CALL_PRIVILEGED"/>
    <uses.permission android:name="android.permission.CAPTURE_AUDIO_OUTPUT"/>
    <uses.permission android:name="android.permission.KILL_BACKGROUND_PROCESSES"/>
    <uses.permission android:name="android.permission.READ_CONTACTS"/>
    <uses.permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses.permission android:name="android.permission.READ_SMS"/>
    <uses.permission android:name="android.permission.READ_SOCIAL_STREAM"/>
    <uses.permission android:name="android.permission.RECEIVE_MMS"/>
    <uses.permission android:name="android.permission.RECEIVE_SMS"/>
    <uses.permission android:name="android.permission.SEND_RESPOND_VIA_MESSAGE"/>
    <uses.permission android:name="android.permission.SEND_SMS"/>
    <uses.permission android:name="android.permission.SET_ACTIVITY_WATCHER"/>
    <uses.permission android:name="android.permission.VIBRATE"/>
    <uses.permission android:name="android.permission.WRITE_CALL_LOG"/>
    <uses.permission android:name="android.permission.WRITE_CONTACTS"/>
    <uses.permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses.permission android:name="android.permission.WRITE_SMS"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:theme="@style/AppTheme"  >
      <application android:theme="@style/AppTheme" > </application>
         <activity android:name="unahhennessy.com.suspend.activity.WelcomeScreen" android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/app_name"  android:theme="@style/FullscreenTheme" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:theme="@style/FullscreenTheme" android:name=".activity.HomeScreen" android:screenOrientation="portrait" />
        <activity android:theme="@style/FullscreenTheme" android:name=".activity.SplashScreen" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:theme="@style/FullscreenTheme" android:name=".activity.Agree_Setup" android:screenOrientation="portrait" />
        <activity android:theme="@style/FullscreenTheme" android:name=".activity.Setup1OneScreen" android:screenOrientation="portrait" />
        <activity android:theme="@style/FullscreenTheme" android:name=".activity.SetUp2TwoScreen" android:screenOrientation="portrait" />
        <activity android:theme="@style/FullscreenTheme" android:name=".activity.SetUp2TwoContinue" android:screenOrientation="portrait" />
        <activity android:theme="@style/FullscreenTheme" android:name=".activity.SetUp2TwoContinue2" android:screenOrientation="portrait" />
        <activity android:theme="@style/FullscreenTheme" android:name=".activity.SuspendOff" android:screenOrientation="portrait" />
        <activity android:theme="@style/FullscreenTheme" android:name=".activity.SuspendOn" android:launchMode="singleTask" android:screenOrientation="portrait" />
        <activity android:theme="@style/FullscreenTheme" android:name=".activity.Help" android:screenOrientation="portrait" />
        <activity android:theme="@style/FullscreenTheme" android:name=".activity.About" android:screenOrientation="portrait" />
        <activity android:theme="@style/FullscreenTheme" android:name=".activity.Settings" android:screenOrientation="portrait" />
        <activity android:theme="@style/FullscreenTheme" android:name=".activity.SetUp1OneEditMessage" android:screenOrientation="portrait" />
        <activity android:theme="@style/FullscreenTheme" android:name=".activity.SettingEditMessage" android:screenOrientation="portrait" />
        <activity android:theme="@style/FullscreenTheme" android:name=".activity.Music" android:screenOrientation="portrait" />
        <activity android:theme="@style/FullscreenTheme" android:name=".activity.Navigation" android:screenOrientation="portrait" />
        <activity android:theme="@style/FullscreenTheme" android:name=".activity.LogScreen" android:screenOrientation="portrait" />
        <receiver android:name=".receiver.PhoneStateReceiver">
            <intent-filter>
                <action android:name="android.intent.action.BOOT_COMPLETED" />
                <action android:name="android.intent.action.PHONE_STATE" />
                <action android:name="android.intent.action.NEW_OUTGOING_CALL" />
            </intent-filter>
        </receiver>
        <receiver android:name=".listener.SmsListener">
            <intent-filter android:priority="2147483647">
                <action android:name="android.provider.Telephony.SMS_RECEIVED" />
            </intent-filter>
        </receiver>
        <receiver android:name=".listener.MmsListener">
            <intent-filter android:priority="2147483647">
                <action android:name="android.provider.Telephony.WAP_PUSH_RECEIVED" />
                <data android:mimeType="application/vnd.wap.mms-message" />
            </intent-filter>
        </receiver>
        <service android:name=".listener.MyPhoneStateListener" />
        <service
            android:name=".listener.PhoneListener"
            android:enabled="true"
            android:exported="true" >
        </service>
    </application>

</manifest>
